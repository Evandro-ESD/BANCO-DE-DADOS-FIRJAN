create database cadastro;
use cadastro;
create table cliente(
	id int not null primary key auto_increment,
    nome varchar(250),
    cpf varchar(11)
);

select * from cliente;

insert into 
	cliente(nome) 
values
	('Frodo');
    
update 
	cliente 
set 
	cpf = '11936045698'
where 
	id = 1;

update 
	cliente
SET 
	nome = 'Frodo Bolseiro'
where 
	id = 1;
    
DELETE FROM cliente WHERE (id = '4444');

    